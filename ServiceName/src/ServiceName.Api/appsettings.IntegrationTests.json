{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },

    "Enrich": [
      "FromLogContext"
    ],

    "Properties": {
      "Application": "ServiceName-IntegrationTests"
    },

    "WriteTo": [
      { "Name": "Console" }
    ]
  },

  "ConnectionStrings": {
    "Default": "Server=localhost;Database=DummyDb;User=sa;Password=Dev@123456;TrustServerCertificate=True"
  },

  "Jwt": {
    "Key": "IntegrationTestKey_12345678901234567890",
    "Issuer": "SampleAuthService.Tests",
    "Audience": "SampleServices.Tests",
    "ExpireMinutes": 30
  },

  "RabbitMq": {
    "HostName": "dummy",
    "Port": 5672,
    "UserName": "dummy",
    "Password": "dummy"
  },

  "Redis": {
    "ConnectionString": "dummy:6379"
  }
}